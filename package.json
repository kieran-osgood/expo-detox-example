{
  "name": "detox-app",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "e2e:build": "detox build --configuration ios",
    "e2e:test": "detox test --configuration ios --workers 2 --watch",
    "e2e": "yarn e2e:build && yarn e2e:test",
    "test": "node_modules/.bin/jest test/**/*.spec.js",
    "e2e:untar": "tar -xzvf build-1660728977605.tar.gz",
    "e2e:install": "xcrun simctl install booted detoxapp.app" 
  },
  "dependencies": {
    "expo": "~46.0.7",
    "expo-asset": "~8.6.1",
    "expo-splash-screen": "~0.16.1",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.4",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "detox": "^19.9.3",
    "detox-expo-helpers": "^0.6.0",
    "detox-recorder": "^1.0.151",
    "expo-detox-hook": "^1.0.10",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-expo": "^46.0.1",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^18.2.0"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo"
  }
}
